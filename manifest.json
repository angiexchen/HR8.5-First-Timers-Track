{
  "name": "Chromemo",  
  "version": "1.0",
  "description" : "Memo that has memes as background depending on what keywords appear throughout the page",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "memo.html",
    "default_icon": "memo.png"
  },

  "icons": {
    "48": "memo.png"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
  
}
